# Gemini VibeCut Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# API Access (choose one)
# =============================================================================

# Option 1: Google AI Studio (for development/prototyping)
GOOGLE_API_KEY=your-google-api-key

# Option 2: Vertex AI (for production)
# USE_VERTEX_AI=true
# GOOGLE_CLOUD_PROJECT=your-project-id
# GOOGLE_CLOUD_LOCATION=us-central1

# =============================================================================
# Model Selection
# =============================================================================

# Use Pro model for better quality (default: false = Flash)
# USE_PRO_MODEL=true

# Model IDs (optional - defaults provided)
GEMINI_MODEL=gemini-3-flash-preview
NANO_BANANA_MODEL=gemini-3-pro-image-preview
VEO_MODEL=veo-3.1-fast-generate-preview

# =============================================================================
# Output Settings
# =============================================================================

OUTPUT_DIR=./assets/outputs

# =============================================================================
# Logging
# =============================================================================

LOG_LEVEL=INFO

# =============================================================================
# External APIs
# =============================================================================

# ElevenLabs (music generation with vocals + lyrics)
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# FAL (Qwen TTS cloud mode â€” optional, torch mode doesn't need this)
# FAL_KEY=key:secret
